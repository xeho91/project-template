{
	"$schema": "https://json.schemastore.org/package",
	"name": "template-project",
	"version": "0.0.0",
	"author": {
		"name": "Mateusz Kadlubowski",
		"email": "xeho91@pm.me",
		"url": "https://xeho91.com/"
	},
	"license": "SEE LICENSE IN LICENSE",
	"homepage": "https://github.com/xeho91/template-project",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xeho91/template-project.git"
	},
	"bugs": {
		"url": "https://github.com/xeho91/template-project/issues"
	},
	"devDependencies": {
		"@ls-lint/ls-lint": "^1.10.0",
		"@types/node": "^16.4.13",
		"@xeho91/commitlint-config": "^1.0.0",
		"@xeho91/eslint-config": "^1.0.0",
		"@xeho91/lint-staged-config": "^1.0.0",
		"@xeho91/markdownlint-config": "^1.0.0",
		"@xeho91/postcss-config": "^1.0.0",
		"@xeho91/prettier-config": "^1.0.0",
		"@xeho91/stylelint-config": "^1.0.0",
		"commitlint": "^13.1.0",
		"eslint": "^7.32.0",
		"gacp": "^2.10.2",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.2",
		"markdownlint": "^0.23.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.3.6",
		"prettier": "^2.3.2",
		"rimraf": "^3.0.2",
		"stylelint": "^13.13.1",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5",
		"vite": "^2.4.4",
		"vite-tsconfig-paths": "^3.3.13"
	},
	"scripts": {
		"build": "tsc && vite build",
		"clean": "rimraf \"dist/\" \".cache/\"",
		"commit": "gacp",
		"dev": "vite --host",
		"format": "prettier --write \".\"",
		"lint": "run-p lint:*",
		"lint:css": "stylelint \"source/**/*.{css,scss,svelte,vue}\"",
		"lint:commits": "commitlint --from \"develop\" --to \"HEAD\"",
		"lint:js": "eslint \"source/**/*.{js,cjs,mjs,ts,jsx,tsx,svelte,vue}\"",
		"lint:ls": "ls-lint",
		"lint:md": "markdownlint \"**/*.md\" --ignore \"./node_modules\"",
		"lint:staged": "lint-staged --verbose",
		"lint:ts": "tsc --project \"tsconfig.json\" --noEmit",
		"prepare": "husky install",
		"preview": "vite preview --host",
		"start": "run-s build preview"
	}
}
